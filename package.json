  "license": "MIT",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "cloc": "^2.3.2",
    "enzyme": "^3.1.0",
    "eslint": "^4.8.0",
    "eslint-plugin-react": "^7.4.0",
    "gh-pages": "^1.0.0",
    "lodash-webpack-plugin": "^0.11.4",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "promise-polyfill": "^6.0.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "clean": "rimraf ./dist ./gh-pages",
    "lint": "eslint index.jsx src/",
    "cloc": "cloc $(git ls-files)",
    "test": "DEBUG=google-book-demo:* DEBUG_DEPTH=5 NODE_ENV=test jest",
    "test:watch": "DEBUG=google-book-demo:* DEBUG_DEPTH=5 NODE_ENV=test yarn test --watch",
    "start": "NODE_ENV=development webpack-dev-server --config=webpack.dev.js --hot --inline --open",
    "prepare": "yarn clean; NODE_ENV=production webpack --config webpack.prod.js",
    "gh-pages": "yarn prepare; mkdir gh-pages; cp -r index.html dist/ gh-pages/; gh-pages -d gh-pages"
  }
}
